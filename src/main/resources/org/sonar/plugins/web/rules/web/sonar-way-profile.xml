<profile>
  <name>Sonar way</name>
  <language>web</language>
  <rules>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>BoldAndItalicTagsCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>ImgWithoutAltCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>PageWithoutTitleCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>ItemTagNotWithinContainerTagCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>AvoidCommentedOutCodeCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>NonConsecutiveHeadingCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>LinkToImageCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>MetaRefreshCheck</key>
    </rule>
    <rule>
      <repositoryKey>Web</repositoryKey>
      <key>ServerSideImageMapsCheck</key>
    </rule>
  </rules>
</profile>
